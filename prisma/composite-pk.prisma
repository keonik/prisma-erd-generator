// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

generator erd {
    provider = "node ./dist/index.js"
    output   = "../__tests__/CompositePk.svg"
    theme    = "forest"
}

model Booking {
    id1          Int
    id2          Int
    inviteeEmail String
    startDateUTC DateTime
    cancelCode   String

    @@id([id1, id2])
}
